# Test Environment Configuration
NODE_ENV=test
PORT=3001

# Auth0 Test Configuration (using test tenant)
AUTH0_DOMAIN=your-tenant.region.auth0.com
AUTH0_CLIENT_ID=your-client-id
AUTH0_CLIENT_SECRET=your-client-secret
AUTH0_AUDIENCE=https://api.your-domain.local

# Test Security Configuration
CORS_ORIGIN=http://localhost:5173
RATE_LIMIT_WINDOW_MS=1000
RATE_LIMIT_MAX_REQUESTS=100

# Test Monitoring Configuration
STATUS_CHECK_INTERVAL=1000
ENABLE_REQUEST_LOGGING=false

# MongoDB is handled by mongodb-memory-server in tests
# MONGO_URI will be set programmatically